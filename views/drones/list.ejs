<h2>Available drones</h2>
<% listDrones.forEach(element => { %>
<div>
  <p>Name: <%= element.name %></p>
  <p>Propellers: <%= element.propellers %></p>
  <p>Maximum Speed: <%= element.maxSpeed %></p>
  <a href="/drones/<%= element.id %>/edit">Edit</a>
  <form action="/drones/<%= element.id %>/delete" method="POST">
    <button type="submit">Delete</button>
  </form>
</div>
<% }) %>
<a href="/drones/create">Create a new Drone</a>
